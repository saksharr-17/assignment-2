#include <stdio.h>

int main() {
    int marks[40], i, n;

    printf("Enter the number of students");
    scanf("%d", &n);

    printf("Enter marks for each student");
    for (i=0;i<n;i++) {
        scanf("%d", &marks[i]);
    }

    for (i=0;i<n;i++) {
        marks[i]+=5;
    }
    printf("\nUpdated marks\n");
    for (i=0;i<n;i++) {
        printf("Student %d: %d\n", i + 1, marks[i]);
    }

    return 0;
}
