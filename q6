#include <stdio.h>

int main() {
    int marks[40], n, i, sum = 0;
    float avg;

    printf("Enter the number of students: ");
    scanf("%d", &n);

    printf("Enter the marks of %d students:\n", n);
    for (i = 0; i < n; i++) {
        scanf("%d", &marks[i]);
        sum += marks[i];
    }

    avg = (float)sum / n;

    printf("Average score: %f\n", avg);

    return 0;
}
