#include <stdio.h>

int main() {
    int marks[40], n, i, sum = 0;

    printf("Enter the number of students: ");
    scanf("%d", &n);

    printf("Enter the marks of %d students:\n", n);
    for (i = 0; i < n; i++) {
        scanf("%d", &marks[i]);
        sum += marks[i];
    }

    printf("Sum of all scores: %d\n", sum);

    return 0;
}
